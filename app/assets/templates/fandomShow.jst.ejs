<h1><a href="javascript:void(0)" class='fandom-index-link'>Fandoms</a> >
<%= category.get("category_name") %></h1>
<% for (var i = 0; i < alphabet.length; i++) { %>
  <div class="row">
    <div class="col-xs-4 col-xs-offset-1">
      <% var letter = alphabet[i] %>
      <%= letter %> <button class="top">Top</button>
      <% for (var j = 0; j < category.get("fandom").length; j++) { %>
        <% var fandom = category.get("fandom")[j] %>
        <% if (fandom.name[0] === letter) { %>
          <div class="show-fandom">
          <a href="javascript:void(0)" class="fandom-list-name">
            <%= fandom.name %>
          </a>
          (<%= fandom.story_count %>)
          </div>
        <% } %>
      <% } %>
    </div>
  </div>
  <p>
<% } %>
